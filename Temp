
while wait() and _G.toggle and game:GetService("RunService").Heartbeat:Wait() do
    for _,v in pairs(game.Workspace:GetChildren()) do
        if v:FindFirstChild("Ki") and v:FindFirstChild("Mesh") then
            v.CFrame = game.Players[Tracker].Character.HumanoidRootPart.CFrame
        end
    end
    for _,c in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
        if c:FindFirstChild("Ki") and c:FindFirstChild("Mesh") then
            c.CFrame = game.Players[Tracker].Character.HumanoidRootPart.CFrame
        end
    end
end
